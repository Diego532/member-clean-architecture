<Application x:Class="Member.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModel="clr-namespace:Member.Presentation.MVVM.ViewModel"
             xmlns:view="clr-namespace:Member.Presentation.MVVM.View"
             xmlns:local="clr-namespace:Member.Presentation"
             >
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewModel:HomeViewModel}">
                <view:HomeView/>
            </DataTemplate>
            
        </ResourceDictionary>
         
    </Application.Resources>
</Application>
